{
  "name": "plural-marketing-site",
  "private": true,
  "engines": {
    "node": "18.12.1"
  },
  "scripts": {
    "dev": "concurrently \"next dev\" \"graphql-codegen --watch --config codegen.ts \" ",
    "build": "next build",
    "start": "next start",
    "lint": "run-p lint:format lint:js lint:css",
    "lint:format": "prettier --check --no-error-on-unmatched-pattern ./{src,pages}/**/*.{js,jsx,ts,tsx,graphql,md,mdpart}",
    "lint:js": "eslint {src,pages} --ext ts,tsx,js,jsx",
    "lint:css": "stylelint src/**/*.css",
    "fix": "run-s fix:format fix:js fix:css",
    "fix:format": "prettier --write --no-error-on-unmatched-pattern ./{src,pages}/**/*.{js,jsx,ts,tsx,graphql,md,mdpart}",
    "fix:js": "eslint --fix {src,pages} --ext ts,tsx,js,jsx",
    "fix:css": "stylelint src/**/*.css --fix",
    "graphql:codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "3.7.12",
    "@docsearch/react": "3.3.3",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@graphql-codegen/add": "4.0.1",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@loomhq/loom-embed": "1.5.0",
    "@markdoc/markdoc": "0.2.2",
    "@markdoc/next.js": "0.2.2",
    "@open-draft/until": "2.1.0",
    "@pluralsh/design-system": "1.337.0",
    "@react-aria/ssr": "3.6.0",
    "@react-stately/list": "3.8.0",
    "@react-stately/select": "3.5.0",
    "@tanstack/react-table": "8.8.5",
    "algoliasearch": "4.17.0",
    "chroma-js": "2.4.2",
    "classnames": "^2.3.2",
    "deep-freeze": "0.0.1",
    "fuse.js": "6.6.2",
    "graphql": "16.6.0",
    "honorable": "0.194.0",
    "honorable-theme-default": "0.77.0",
    "immer": "9.0.21",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "memoize-one": "6.0.0",
    "next": "13.3.0",
    "next-compose-plugins": "2.2.1",
    "next-transpile-modules": "10.0.0",
    "posthog-js": "1.53.4",
    "raw-loader": "4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-embed": "3.6.0",
    "react-github-btn": "1.4.0",
    "react-is": "18.2.0",
    "react-spring": "9.7.1",
    "react-transition-group": "4.4.5",
    "react-use-measure": "2.1.1",
    "rooks": "7.11.2",
    "styled-components": "5.3.10",
    "swr": "2.1.3",
    "tslib": "2.5.0",
    "usehooks-ts": "2.9.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/client-preset": "3.0.1",
    "@pluralsh/eslint-config-typescript": "2.5.41",
    "@pluralsh/stylelint-config": "1.1.3",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "autoprefixer": "10.4.14",
    "concurrently": "8.0.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-newlines": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.2.7",
    "react-hooks": "1.0.1",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.5",
    "tailwindcss": "3.3.1",
    "typescript": "4.9.5"
  },
  "packageManager": "yarn@3.3.0",
  "lint-staged": {
    "./{src,pages}/**/*.{js,jsx,ts,tsx,graphql,md,mdpart}": "prettier --write"
  }
}
